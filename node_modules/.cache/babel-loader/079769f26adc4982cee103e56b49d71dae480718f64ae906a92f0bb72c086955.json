{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductCard from '../ProductCard.vue';\nimport FilterComp from '../filter/FilterComp.vue';\nimport FooterSection from '../Footer.vue';\nimport HeaderSection from '../Header.vue';\nimport BtnPage from '../Button.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    ProductCard,\n    FilterComp,\n    FooterSection,\n    HeaderSection,\n    BtnPage\n  },\n  props: {\n    productList: []\n  },\n  data() {\n    return {\n      filterList: 'empty',\n      currentPage: 1\n    };\n  },\n  methods: {\n    async filterCatalog(e) {\n      try {\n        this.currentPage = 1;\n        let filterApi = `http://localhost:80/karigui/rest-api/rest/api/V1/products.php?name=${e.name}&size=${e.size}&color=${e.color}&gender=${e.gender}&price=${e.price}&type=${e.type}`;\n        let response = await fetch(filterApi);\n        let products = await response.json();\n        let count = 1;\n        this.filterList = [];\n        for (let i = 0; i < products.length; i += 25) {\n          let tempArray = [];\n          for (let j = i; j < i + 25; j++) {\n            if (j > products.length - 1) {\n              break;\n            }\n            tempArray.push(products[j]);\n          }\n          this.filterList[count] = tempArray;\n          count++;\n        }\n        console.log(this.filterList);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    show() {\n      this.filterList = this.productList;\n    },\n    test() {\n      console.log(this.productList);\n    },\n    update(number) {\n      this.currentPage = number;\n    }\n  }\n};","map":{"version":3,"names":["ProductCard","FilterComp","FooterSection","HeaderSection","BtnPage","name","components","props","productList","data","filterList","currentPage","methods","filterCatalog","e","filterApi","size","color","gender","price","type","response","fetch","products","json","count","i","length","tempArray","j","push","console","log","error","show","test","update","number"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/karigui/src/components/Page/homePage.vue"],"sourcesContent":["<template>\n    <header-section />\n    <section class=\"home-page\">\n    <button @click=\"test\">Test</button>\n        <div class=\"filter\">\n            <filter-comp v-model=\"filterList\" @filter=\"filterCatalog\"/>\n        </div>\n        <div class=\"container\">\n            <div class=\"catalog\" v-if=\"this.filterList == 'empty'\">\n                <product-card  v-for=\"product in productList[this.currentPage]\" :key=\"product.productId\" :product=\"product\"/>\n            </div>\n            <div class=\"catalog\" v-else>\n                <product-card  v-for=\"product in this.filterList[this.currentPage]\" :key=\"product.productId\" :product=\"product\"/>\n            </div>\n            <div class=\"btnPage\" v-if=\"this.filterList == 'empty'\">\n                <BtnPage @pageUpdate=\"update\" v-for=\"( product , index ) in productList\" :key=\"product\" :number=\"index+1\"/>\n            </div>\n            <div class=\"btnPage\" v-else>\n                <BtnPage @pageUpdate=\"update\" v-for=\"( product , index ) in filterList\" :key=\"product\" :number=\"index+1\"/>\n            </div>\n            <footer-section />\n        </div>\n    </section>\n</template>\n\n<script>\nimport ProductCard from '../ProductCard.vue';\nimport FilterComp from '../filter/FilterComp.vue';\nimport FooterSection from '../Footer.vue';\nimport HeaderSection from '../Header.vue';\nimport BtnPage from '../Button.vue';\n\nexport default{\n    name:'HomePage',\n    components:{\n        ProductCard,\n        FilterComp,\n        FooterSection,\n        HeaderSection,\n        BtnPage\n    },\n    props:{\n        productList:[]\n    },\n    data(){\n        return{\n            filterList: 'empty',\n            currentPage: 1\n        }\n    },\n    methods:{\n        async filterCatalog(e){\n            try{\n                this.currentPage = 1;\n                let filterApi = `http://localhost:80/karigui/rest-api/rest/api/V1/products.php?name=${e.name}&size=${e.size}&color=${e.color}&gender=${e.gender}&price=${e.price}&type=${e.type}`;\n                let response = await fetch(filterApi);\n                let products = await response.json();\n                let count = 1;\n                this.filterList = [];\n                for(let i = 0; i < products.length; i+=25) {\n                let tempArray = []\n                for (let j = i; j < (i + 25); j++) {\n                    if ( j > products.length-1 ) {\n                    break;\n                    }\n                    tempArray.push(products[j]);\n                }\n                this.filterList[count] = tempArray;\n                count++;\n                }\n                console.log(this.filterList)\n            }catch(error){\n                console.log(error)\n            }\n        },\n        show(){\n            this.filterList = this.productList;\n        },\n        test(){\n            console.log(this.productList);\n        },\n        update(number){\n            this.currentPage = number\n        }\n    }\n}\n</script>\n"],"mappings":";AA0BA,OAAOA,WAAU,MAAO,oBAAoB;AAC5C,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,aAAY,MAAO,eAAe;AACzC,OAAOC,aAAY,MAAO,eAAe;AACzC,OAAOC,OAAM,MAAO,eAAe;AAEnC,eAAc;EACVC,IAAI,EAAC,UAAU;EACfC,UAAU,EAAC;IACPN,WAAW;IACXC,UAAU;IACVC,aAAa;IACbC,aAAa;IACbC;EACJ,CAAC;EACDG,KAAK,EAAC;IACFC,WAAW,EAAC;EAChB,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,UAAU,EAAE,OAAO;MACnBC,WAAW,EAAE;IACjB;EACJ,CAAC;EACDC,OAAO,EAAC;IACJ,MAAMC,aAAaA,CAACC,CAAC,EAAC;MAClB,IAAG;QACC,IAAI,CAACH,WAAU,GAAI,CAAC;QACpB,IAAII,SAAQ,GAAK,sEAAqED,CAAC,CAACT,IAAK,SAAQS,CAAC,CAACE,IAAK,UAASF,CAAC,CAACG,KAAM,WAAUH,CAAC,CAACI,MAAO,UAASJ,CAAC,CAACK,KAAM,SAAQL,CAAC,CAACM,IAAK,EAAC;QACjL,IAAIC,QAAO,GAAI,MAAMC,KAAK,CAACP,SAAS,CAAC;QACrC,IAAIQ,QAAO,GAAI,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;QACpC,IAAIC,KAAI,GAAI,CAAC;QACb,IAAI,CAAC